{
  "name": "gun-avatar",
  "description": "Avatar generator for GUN public keys",
  "version": "1.6.1",
  "author": {
    "name": "Davay",
    "email": "davay@frkt.ru"
  },
  "repository": "github:DeFUCC/gun-avatar",
  "bugs": {
    "url": "https://github.com/DeFUCC/gun-avatar/issues",
    "email": "davay@frkt.ru"
  },
  "license": "MIT",
  "keywords": [
    "gun",
    "sea",
    "public key",
    "visualizer",
    "avatar"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/gun-avatar.umd.js",
  "module": "./dist/gun-avatar.es.js",
  "exports": {
    ".": {
      "import": "./dist/gun-avatar.es.js",
      "require": "./dist/gun-avatar.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "page": "vite build --mode page",
    "serve": "vite preview --port=5005",
    "deploy": "sh deploy.sh"
  },
  "dependencies": {
    "@vueuse/core": "7.6.1",
    "vue": "^3.2.30",
    "gun": "^0.2020.1235"
  },
  "devDependencies": {
    "@iconify/json": "^2.0.36",
    "@vitejs/plugin-vue": "^2.2.0",
    "pug": "3.0.2",
    "unplugin-icons": "^0.13.1",
    "unplugin-vue-components": "^0.17.18",
    "vite": "^2.8.1",
    "vite-plugin-windicss": "^1.7.0",
    "windicss": "^3.4.3",
    "vitepress": "^0.22.0"
  }
}
