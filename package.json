		"tsup": "^8.0.1",
		"typescript": "^5.3.3",
		"unocss": "^0.58.2",
		"unplugin-icons": "^0.18.1",
		"vite": "^5.0.10",
		"vitepress": "^1.0.0-rc.34"
	},
	"tsup": {
		"entry": [
			"src/index.ts"
		],
		"splitting": false,
